  pages = {187-201},
  keywords = {14M25,13A02,20M25,Affine monoid ring,Primary 13C05,Secondary 05E40,Segre product,Toric fiber product,Toric ideal,Veronese ring},
  author = {Kahle, Thomas and Rauh, Johannes},
